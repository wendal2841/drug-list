{
    "rules": {
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import |^export {(.*?)}|class [a-zA-Z]+ implements |interface [a-zA-Z]+ extends |^`[a-zA-Z]+|"
            }
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-any": true,
        "no-trailing-whitespace": [
            true,
            "ignore-blank-lines"
        ],
        "no-submodule-imports": false,
        "no-implicit-dependencies": [
            true,
            "dev",
            [
                "pages",
                "types"
            ]
        ],
        "ordered-imports": false,
        "member-access": [
            true,
            "no-public"
        ],
        "no-empty-interface": false,
        "whitespace": [
            true,
            "check-module",
            "check-branch",
            "check-decl",
            "check-operator",
            "check-rest-spread",
            "check-type-operator",
            "check-preblock",
            "check-type"
        ],
        "no-unused-expression": false,
        "no-this-assignment": false,
        "no-console": false,
        "comment-format": false,
        "object-literal-sort-keys": false,
        "no-empty": false,
        "variable-name": [true, "check-format", "allow-leading-underscore", "allow-pascal-case"],
        "max-classes-per-file": false,
        "interface-name": false,
        "no-namespace": false,
        "typedef": [true, "call-signature", "arrow-call-signature"],
        "newline-before-return": true,
        "object-curly-spacing": [true, "always"],
        "no-consecutive-blank-lines": true,
        "indent": [true, "spaces", 4],
        "no-default-export": true
    },
    "rulesDirectory": []
}
